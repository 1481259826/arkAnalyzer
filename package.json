{
  "name": "arkanalyzer",
  "version": "1.0.8",
  "files": [
    "docs",
    "lib",
    "config"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "prepack": "tsc -p ./tsconfig.prod.json",
    "test": "vitest",
    "testonce": "vitest --no-color run",
    "coverage": "vitest run --coverage",
    "heapdump": "npm run build && node --expose-gc out/tests/HeapDumpTest.js",
    "gendoc": "npx typedoc",
    "postinstall": "node script/npmInstall.js",
    "copyCollectionDefintion": "ts-node script/typescriptCollectionDefinitionCopy.ts",
    "demo": "ts-node --transpileOnly tests/ex01-03/quickstart.ts",
    "demo:simple": "ts-node --transpileOnly tests/ex04/testSimpleDemo.ts",
    "demo:countdown": "ts-node --transpileOnly tests/ex04/testCountDown.ts"
  },
  "dependencies": {
    "commander": "13.1.0",
    "log4js": "6.9.1",
    "ohos-typescript": "^4.9.5-r4"
  },
  "bundledDependencies": [
    "ohos-typescript"
  ],
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@vitest/coverage-v8": "^1.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "~4.9.5",
    "vitest": "^1.5.0"
  }
}
